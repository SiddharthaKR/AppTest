<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="already_hav">
        <item name="android:textSize">
            19sp
        </item>
        <item name="android:textColor">
            #FFFFFF
        </item>
    </style>
    <style name="or">
        <item name="android:textSize">
            18sp
        </item>
        <item name="android:textColor">
            #FFFFFF
        </item>
    </style>
    <style name="password">
        <item name="android:textSize">
            20sp
        </item>
        <item name="android:textColor">
            #B7B7B7
        </item>
    </style>
    <style name="email_addre">
        <item name="android:textSize">
            20sp
        </item>
        <item name="android:textColor">
            #B7B7B7
        </item>
    </style>
    <style name="name">
        <item name="android:textSize">
            20sp
        </item>
        <item name="android:textColor">
            #B7B7B7
        </item>
    </style>
    <style name="create_an_a">
        <item name="android:textSize">
            30sp
        </item>
        <item name="android:textColor">
            #FFFFFF
        </item>
    </style>

    <style name="continue_wi">
        <item name="android:textSize">
            18sp
        </item>
        <item name="android:textColor">
            #000000
        </item>
    </style>

    <style name="create_an_account">
        <item name="android:textSize">
            18sp
        </item>
        <item name="android:textColor">
            #FFFFFF
        </item>
    </style>

    <style name="order_summary_heading_2">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:fontFamily">@font/clash_display_bold</item>
        <item name="android:layout_marginBottom">16dp</item>
    </style>

    <style name="order_summary_small_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/TextGrey</item>
        <item name="android:textSize">16sp</item>
        <item name="android:fontFamily">@font/hk_grotesk</item>
    </style>

    <style name="order_summary_medium_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">18sp</item>
        <item name="android:fontFamily">@font/hk_grotesk</item>
    </style>

    <style name="bottom_orange_button">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:fontFamily">@font/clash_display_bold</item>
        <item name="android:textSize">18dp</item>
        <item name="android:background">@drawable/bottom_orange_button_bg</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:padding">16.5dp</item>
    </style>

    <style name="order_summary_very_small_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">12dp</item>
        <item name="android:textColor">#BCBCBC</item>
        <item name="android:fontFamily">@font/hk_grotesk</item>
        <item name="android:textSize">14dp</item>
    </style>

    <style name="merch_size_selected">
        <item name="android:background">@drawable/merch_size_btn_selected</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:layout_margin">5dp</item>
        <item name="android:textSize">18sp</item>
        <item name="android:layout_width">50dp</item>
        <item name="android:layout_height">50dp</item>
    </style>

    <style name="merch_size_deselected">
        <item name="android:layout_margin">5dp</item>
        <item name="android:background">@drawable/merch_size_btn_deselect</item>
        <item name="android:layout_width">50dp</item>
        <item name="android:textSize">18sp</item>
        <item name="android:layout_height">50dp</item>
    </style>
    <style name="merch_size_not_avail">
        <item name="android:layout_margin">5dp</item>
        <item name="android:background">@drawable/not_avail_btn</item>
        <item name="android:textColor">#707683</item>
        <item name="android:layout_width">50dp</item>
        <item name="android:textSize">18sp</item>
        <item name="android:layout_height">50dp</item>
    </style>

    <style name="cart_count_icon">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:minWidth">20dp</item>
        <item name="android:minHeight">16dp</item>
        <item name="android:layout_marginStart">14dp</item>
        <item name="android:layout_marginBottom">12dp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:background">@drawable/cart_count_bg</item>
        <item name="android:textAlignment">center</item>
        <item name="android:textSize">14sp</item>
        <item name="android:paddingStart">4dp</item>
        <item name="android:paddingEnd">4dp</item>
        <item name="android:paddingTop">2dp</item>
        <item name="android:paddingBottom">2dp</item>
        <item name="android:visibility">gone</item>
        <item name="android:fontFamily">@font/clashdisplay</item>
    </style>

    <style name="signup_edit_text">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">320dp</item>
        <item name="android:layout_margin">4dp</item>
        <item name="android:paddingTop">10dp</item>
        <item name="android:paddingBottom">10dp</item>
        <item name="android:paddingStart">20dp</item>
        <item name="android:paddingEnd">20dp</item>
        <item name="android:textSize">20sp</item>
        <item name="android:fontFamily">@font/hk_grotesk</item>
        <item name="android:textColorHint">#B7B7B7</item>
        <item name="android:background">@drawable/custom_edit_text</item>
        <item name="android:layout_gravity">center</item>
    </style>

    <style name="select_interest_item">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">36dp</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">18sp</item>
        <item name="android:fontFamily">@font/hk_grotesk</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/interests</item>
        <item name="android:layout_marginTop">2dp</item>
        <item name="android:layout_marginBottom">2dp</item>
        <item name="android:layout_marginStart">4dp</item>
        <item name="android:layout_marginEnd">4dp</item>
        <item name="android:paddingTop">3dp</item>
        <item name="android:paddingBottom">5dp</item>
        <item name="android:paddingStart">14dp</item>
        <item name="android:paddingEnd">14dp</item>
        <item name="android:onClick">interestItemClick</item>
    </style>

    <style name="select_interest_item_profile">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">36dp</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">18sp</item>
        <item name="android:fontFamily">@font/hk_grotesk</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/interests</item>
        <item name="android:layout_marginTop">2dp</item>
        <item name="android:layout_marginBottom">2dp</item>
        <item name="android:layout_marginStart">4dp</item>
        <item name="android:layout_marginEnd">4dp</item>
        <item name="android:paddingTop">3dp</item>
        <item name="android:paddingBottom">5dp</item>
        <item name="android:paddingStart">14dp</item>
        <item name="android:paddingEnd">14dp</item>
        <item name="android:onClick">interestItemOnClick</item>
    </style>

    <style name="interests_horizontal_container">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_marginTop">8dp</item>
    </style>
</resources>

